<body>
    <!--? Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <img src="/assets/img/logo/logo.png" alt="">
                </div>
            </div>
        </div>
    </div>
    
    <div class="logo" style="position: absolute; left: 50px; top: 10px;">
        <a href="/"><img src="/assets/img/logo/logo.png" alt=""></a>
    </div>
      <main>

                        <div class="row align-items-center">
                            <!-- Logo -->
                            <div class="col-xl-2 col-lg-2 col-md-1">
                                
                            </div>
                        
             
        
  
     <br>
                            
        <div id=box>
            <br>
            
            <h4 class="widget_title" style="color: #2d2d2d;"><%=currentuser.displayName%></h4>
            <br>
               <div id=controls>
                   
                <div id="editicon"><a href="google">
                    <span class="glyphicon glyphicon-edit"></span> Edit
                  </a>
                </div>
                <br>
                <br>
            </div>
                <table>   
                    <tr>
                        <td style="padding-right: 10%;"><label for="#username">Username:</label></td>
                        <td>
                            <div class="form-group">
                                <input type="email" class="form-control"  id="username" name="username" placeholder='<%= currentuser.username%>'' readonly required>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="#firstname">First Name:</label></td>
                        <td>
                            <div class="form-group">
                                <input type="text" class="form-control" id="firstname" name="firstname" placeholder='<%= currentuser.firstname%>' readonly required>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="#lastname">Last Name:</label></td>
                        <td>
                            <div class="form-group">
                                <input type="text" class="form-control" id="lastname" name="lastname" placeholder='<%= currentuser.lastname%>' readonly  required>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="#mobile">Mobile:</label></td>
                        <td>
                            <div class="form-group">
                                <input type="number" class="form-control" id="mobile" name="mobile" placeholder='<%= currentuser.mobileno%>' minlength="10" maxlength="10" readonly  required>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="#userphoto">User Photo:</label></td>
                        <% if(currentuser.userphoto==''){%>
                        <td>    
                            <div class="form-group">
                                <input type="file" class="form-control" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter email'" id="userphoto" name="userphoto" placeholder='Enter email' disabled required>
                            </div>
                        </td>
                        <%}%>
                    </tr>               
                </table>
                <% if(currentuser.userphoto!=''){ console.log(currentuser.userphoto);%>
                <a href='<%=currentuser.userphoto%>'> <img src ='<%=currentuser.userphoto%>' height="200" width="200"> </a>  
                    
                <%}%>
           
            <br>
            <br>
            <br>           
        </div>
          </div>
